hello = 'hello world'
name = 'Bogdan'
prizv = 'matiushko'
age = '16'

print(hello, type(hello))
print(name, type(name))
print(prizv, type(prizv))
print(age, type(age))

list = [hello, name, prizv, age]

if all(type(var) == type(list[0]) for var in list):
    print("good")
else:
    print("Типи відрізняються")
